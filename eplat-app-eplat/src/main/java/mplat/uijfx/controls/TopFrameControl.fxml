<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.*?>

<fx:root type="javafx.scene.layout.HBox" fx:id="topFrame" alignment="CENTER_LEFT" prefHeight="125.0" prefWidth="1500.0" spacing="10.0" styleClass="TopFrameView" xmlns:fx="http://javafx.com/fxml">
  <children>
    <ImageView fx:id="imgCourseWare" fitHeight="70.0" fitWidth="70.0" opacity="0.5" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@../images/btn-course-ware.png" />
      </image>
    </ImageView>
    <ImageView fx:id="imgTopicTrain" fitHeight="70.0" fitWidth="70.0" opacity="0.5" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@../images/btn-topic-train.jpg" />
      </image>
    </ImageView>
    <ImageView fx:id="imgEmergeTrain" fitHeight="70.0" fitWidth="70.0" opacity="0.5" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@../images/btn-emerge-train.jpg" />
      </image>
    </ImageView>
    <ImageView fx:id="imgEmergeExam" fitHeight="70.0" fitWidth="70.0" opacity="0.5" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@../images/btn-emerge-exam.jpg" />
      </image>
    </ImageView>
    <ImageView fx:id="imgSystemCfg" fitHeight="70.0" fitWidth="70.0" opacity="0.5" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@../images/btn-system-cfg.jpg" />
      </image>
    </ImageView>
    <StackPane alignment="CENTER_RIGHT" prefHeight="120.0" HBox.hgrow="ALWAYS">
      <children>
        <ImageView fx:id="imgGotoMain" fitHeight="70.0" fitWidth="70.0" onMouseClicked="#onMouseClick" onMouseEntered="#onMouseOn" onMouseExited="#onMouseOut" opacity="0.5" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@../images/return.gif" />
          </image>
        </ImageView>
      </children>
    </StackPane>
  </children>
  <padding>
    <Insets left="20.0" right="20.0" />
  </padding>
  <stylesheets>
    <URL value="@TopFrameView.css" />
  </stylesheets>
</fx:root>
